{
  "locals": {
    "url": "http://localhost:8080",
    "name": "slate-css",
    "version": "0.1.0-alpha.11"
  },
  "plugins": [
    "./plugins/slate.coffee",
    "wintersmith-ejs"
  ],
  "sassify": {
    "outputStyle": "compressed",
    "sourceComments": "false",
    "sourceMap": true
  },
  "require": {
    "_": "underscore"
  },
  "markdown": {
    "smartLists": true,
    "smartypants": true
  }
}
